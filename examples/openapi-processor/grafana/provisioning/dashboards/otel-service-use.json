    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Current container ID from service metrics",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "string"
        }
      },
      "gridPos": { "h": 5, "w": 4, "x": 20, "y": 3 },
      "id": 8,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "/^container_id$/", "values": false },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "count by (container_id) (${namespace}http_server_request_count_total{service_name=\"${service}\", span_kind=\"SPAN_KIND_SERVER\"})",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Container ID",
      "transformations": [
        { "id": "organize", "options": { "excludeByName": { "Time": true, "Value": true } } }
      ],
      "type": "stat"
    },
    {
      "collapsed": false,
