      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "(sum(rate(${namespace}http_server_request_count_total{span_kind=\"SPAN_KIND_SERVER\",service_name=\"${service}\", http_response_status_code=~\"5..\"}[$__rate_interval])) or vector(0)) / sum(rate(${namespace}http_server_request_count_total{span_kind=\"SPAN_KIND_SERVER\",service_name=\"${service}\"}[$__rate_interval]))",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
