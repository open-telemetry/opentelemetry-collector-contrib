      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{id=~\"/docker/${container_id}.*\"}[$__rate_interval])) * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "title": "CPU Utilization",
      "type": "gauge"
    },
